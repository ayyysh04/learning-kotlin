<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/homePageFragment">

    <fragment
        android:id="@+id/emailFragment2"
        android:name="com.example.myapplication.navigationComponents.dataBinding.EmailFragment"
        android:label="EmailFragment"
        tools:layout="@layout/fragment_email2">
        <action
            android:id="@+id/action_emailFragment2_to_welcomeFragment2"
            app:destination="@id/welcomeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/homePageFragment"
        android:name="com.example.myapplication.navigationComponents.dataBinding.HomePageFragment"
        android:label="HomePageFragment"
        tools:layout="@layout/fragment_homepage2">
        <action
            android:id="@+id/action_homePageFragment_to_nameFragment2"
            app:destination="@id/nameFragment2" />
        <action
            android:id="@+id/action_homePageFragment_to_termsFragment2"
            app:destination="@id/termsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/nameFragment2"
        android:name="com.example.myapplication.navigationComponents.dataBinding.NameFragment"
        android:label="NameFragment"
        tools:layout="@layout/fragment_name2">
        <action
            android:id="@+id/action_nameFragment2_to_emailFragment2"
            app:destination="@id/emailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment2"
        android:name="com.example.myapplication.navigationComponents.dataBinding.WelcomeFragment"
        android:label="WelcomeFragment"
        tools:layout="@layout/fragment_welcome2">
        <action
            android:id="@+id/action_welcomeFragment2_to_termsFragment2"
            app:destination="@id/termsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/termsFragment2"
        android:name="com.example.myapplication.navigationComponents.viewBinding.TermsFragment"
        android:label="fragment_terms"
        tools:layout="@layout/fragment_terms2" />
</navigation>